        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@fullHeight) and not (@showTitle) and not (@showSubtitle)": {
              "padding": "0",
              ".card-img, .card-img img, .card-img amp-img, .card-box": {
                "min-height": "450px",
                "@media (max-width: 991px)": {
                  "min-height": "350px"
                }
              },
              ".card-box when not (@showImg)": {
                "@media (min-width: 992px)": {
                  "min-height": "900px"
                }
              },
              ".card-box when not (@showImg) and (@transparentBg) and not (@showBorders)": {
                "min-height": "270px"
              }
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-img when (@showImg)": {
              "padding": "0"
            },
            "& when (@showImg) and (@transparentBg) and not (@showBorders)": {
              ".card-wrapper when (@showCardsTitle), (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
                ".card-img": {
                  "@media (max-width: 767px)": {
                    "width": "100%",
                    "margin-bottom": "30px"
                  }
                },
                "@media (min-width: 768px)": {
                  ".card-box": {
                    "margin-left": "15px"
                  },
                  "&.mbr-row-reverse": {
                    ".card-box": {
                      "margin-right": "15px",
                      "margin-left": "0px"
                    }
                  }
                }
              }
            },
            ".card-benefits-box when (@showCardsTitleNumber)": {
              "align-items": "center",
              ".card-number when (@showLineNumber) and (@showCardsNumber)": {
                "position": "relative",
                "padding-left": "70px",
                "&:before": {
                  "content": "''",
                  "width": "50px",
                  "height": "1px",
                  "position": "absolute",
                  "left": "10px",
                  "top": "50%",
                  "transform": "translateY(-50%)",
                  "background-color": "@lineColor"
                }
              }
            },
            ".card-wrapper, .card-wrapper .card-box": {
              "position": "relative"
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-box when (@showCardsTitleNumber), (@showCardsTitle), (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
              "width": "100%",
              "& when (@showBorders), not (@transparentBg)": {
                "padding": "60px",
                "@media (max-width: 991px)": {
                  "padding": "25px"
                }
              }
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-wrapper when (@showCardsEffect)": {
              "& when (@showCardsTitleNumber), (@showCardsTitle)": {
                "& when (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
                  "&:hover when not (@transparentBg), (@showBorders)": {
                    "& when (@showBorders) and (@transparentBg)": {
                      "background-color": "@primaryColor"
                    },
                    "> *, .card-box > *, .card-text-box > *, .card-box .btn[class*=\"-outline\"], .card-benefits-box > *": {
                      "-webkit-transition": "all 0.3s linear",
                      "transition": "all 0.3s linear"
                    },
                    ".mbr-overlay": {
                      "background": "@primaryColor"
                    },
                    ".card-benefits-box > *, .card-text-box > *": {
                      "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                    },
                    ".card-box .btn[class*=\"-outline\"], .card-box .btn[class*=\"-outline\"]:hover": {
                      "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important",
                      "border-color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                    }
                  }
                }
              }
            },
            ".mbr-row .card when not (@showPadding)": {
              "padding": "0",
              "& when (@showBorders)": {
                ".card-wrapper": {
                  "border": "none"
                },
                "@media (min-width: 992px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "1px solid @bordersColor"
                  },
                  "&.lg-pb .card-wrapper when (@cardsAmount > 2)": {
                    "border-bottom": "1px solid @bordersColor"
                  },
                  "&.lg-pb:nth-child(2) .card-wrapper when (@cardsAmount > 2)": {
                    "border-right": "none"
                  }
                },
                "@media (max-width: 991px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "none",
                    "border-bottom": "1px solid @bordersColor"
                  }
                }
              }
            }
          },
          "_name": "features9",
          "_customHTML": "<section class=\"features9\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n            <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"!(fullHeight &amp;&amp; !showTitle &amp;&amp; !showSubtitle)\">\n            <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"!(fullHeight &amp;&amp; !showTitle &amp;&amp; !showSubtitle)\">\n            <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n            <input type=\"checkbox\" title=\"Cards Full Height\" name=\"fullHeight\" condition=\"!showTitle &amp;&amp; !showSubtitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Chess Style\" name=\"chessStyle\" condition=\"showImg\" checked=\"\">\n        <header>Title</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n            <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n                <option value=\"1\">Left</option>\n                <option value=\"2\" selected=\"\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n        \n        <header>Cards</header>\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n            <input type=\"checkbox\" title=\"Show Title Number\" name=\"showCardsTitleNumber\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Number\" name=\"showCardsNumber\" checked=\"\" condition=\"showCardsTitleNumber\">\n            <input type=\"checkbox\" title=\"Show Line Before Number\" name=\"showLineNumber\" condition=\"showCardsTitleNumber &amp;&amp; showCardsNumber\" checked=\"\">\n            <input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#9ea1a9\" condition=\"showCardsTitleNumber &amp;&amp; showCardsNumber &amp;&amp; showLineNumber\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showCardsSubtitle\">\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Buttons\" name=\"showCardsButtons\">\n            <select title=\"Align\" name=\"cardsAlign\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\">\n                <option value=\"1\" selected=\"\">Left</option>\n                <option value=\"2\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n            <input type=\"checkbox\" title=\"Show Image\" name=\"showImg\" checked=\"\">\n            <input type=\"range\" inline=\"\" title=\"Image Width\" name=\"imgWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"showImg\">\n            <input type=\"checkbox\" title=\"Show Effect\" name=\"showCardsEffect\" condition=\"(showCardsTitleNumber || showCardsTitle) &amp;&amp; (showCardsSubtitle || showCardsText || showCardsButtons) &amp;&amp; (showBorders || !transparentBg)\" checked=\"\">\n            <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showImg\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Padding\" name=\"showPadding\" condition=\"showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n            <input type=\"checkbox\" title=\"Borders\"
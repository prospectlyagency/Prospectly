            },
            ".card, .slider-box, amp-carousel, .dots-wrapper": {
              "width": "100%"
            },
            ".dots span, .amp-carousel-button": {
              "background-color": "@buttonsColor"
            },
            "& when not (@typeSlide = 'carousel')": {
              ".card": {
                "height": "~\"calc(@{slideHeight}px - 35px)\"",
                "margin-bottom": "35px"
              },
              ".dots-wrapper": {
                "position": "absolute",
                "bottom": "0"
              },
              ".dots span": {
                "display": "inline-block",
                "vertical-align": "middle",
                "border-radius": "10px",
                "width": "9px",
                "height": "9px",
                "margin": "0 3px",
                "opacity": "0.4",
                "outline": "none"
